{
  "name": "wilmat-web-page",
  "private": true,
  "description": "Wilmat s.c. web page",
  "version": "0.1.0",
  "author": "Aleksander Fret",
  "dependencies": {
    "gatsby": "2.23.11",
    "gatsby-image": "2.4.9",
    "gatsby-plugin-manifest": "2.4.14",
    "gatsby-plugin-offline": "3.2.13",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.3.6",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sass": "2.3.6",
    "gatsby-plugin-sharp": "2.6.14",
    "gatsby-plugin-sitemap": "2.4.7",
    "gatsby-source-filesystem": "2.3.14",
    "gatsby-transformer-sharp": "2.5.7",
    "gatsby-transformer-yaml": "2.4.6",
    "gatsby-transformer-yaml-full": "0.3.3",
    "gatsby-yaml-full-import": "0.3.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "sass": "1.26.9"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.1",
    "eslint-plugin-react-hooks": "4.0.4",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-stylelint": "3.2.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "stylelint": "13.6.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-scss": "3.18.0",
    "stylelint-webpack-plugin": "2.1.0"
  },
  "scripts": {
    "build": "gatsby build",
    "check": "npm-run-all lint format stylelint",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --check \"src/**/*.{js,jsx,json,md,html}\"",
    "hooks": "node scripts/enhanceGitHooks.js",
    "lint": "eslint . --ext \".js,.jsx\"",
    "postinstall": "npm run hooks",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "stylelint": "stylelint src/**/*.scss --cache",
    "test": "echo Write tests"
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/alemikolo/wilmat-2020"
  },
  "bugs": {
    "url": "https://https://github.com/alemikolo/wilmat-2020/issues"
  },
  "browserslist": [
    ">0.5%",
    "not ie < 9"
  ],
  "lint-staged": {
    "*.{js,jsx,json,scss,css,md,html}": "prettier --write",
    "*.{js,jsx}": [
      "eslint -c .eslintrc --fix",
      "echo test"
    ],
    "*.scss": "stylelint --syntax scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check"
    }
  }
}
